<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <ApplicationIcon>bin\Release\net7.0\taxi_icon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="bin\Release\net7.0\taxi_icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="GemBox.Imaging" Version="10.0.1058" />
    <PackageReference Include="SQPhoto" Version="1.0.2" />
    <PackageReference Include="Vintasoft.Imaging" Version="12.1.20.1" />
    <PackageReference Include="Zeroit.Framework.PictureBox" Version="1.0.0" />
  </ItemGroup>

</Project>
